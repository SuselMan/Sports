{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "target": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "types": ["react", "react-native"]
  }
}

